{
  "trailingSlash":true,
  "rewrites": [    
    {
      "source": "/journal/:match*", 
      "destination": "https://next-proxy-test.ghost.io/journal/:match*" 
    }
  ],
  "headers": [
    {
      "source": "/journal/:path*",
      "headers": [
        { 
          "key": "x-forwarded-host", 
          "value": "next-proxy-test.vercel.app" 
        }
      ]
    }
  ]
}
